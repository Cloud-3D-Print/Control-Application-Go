[Unit]
Description=TPCPI Remote Server
After=network-online.target syslog.target

[Service]
Type=simple
WorkingDirectory=/home/pi/control/piPrinterCtrl
User=pi
ExecStart=/home/pi/control/piPrinterCtrl/piPrinterCtrl
Restart=on-failure
LimitNOFILE=65536
RestartSec=5s
TimeoutSec=0
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=piPrinterCtrl

[Install]
WantedBy=multi-user.target
